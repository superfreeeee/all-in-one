{
  "name": "all-in-one",
  "private": true,
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "prepare": "husky install",
    "test": "pnpm --filter=* run test",
    "test:coverage": "pnpm --filter=* run test:coverage",
    "test:all-in-one": "pnpm --filter=* run test",
    "test:all-in-one:coverage": "pnpm --filter=all-in-one run test:coverage",
    "typechecking": "pnpm --filter=all-in-one run typechecking"
  },
  "devDependencies": {
    "husky": "^8.0.3"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "vue-frag>vue": "^3"
      }
    },
    "allowedDeprecatedVersions": {
      "@babel/plugin-proposal-class-properties": "7.18.6",
      "abab": "2.0.6",
      "consolidate": "0.15.1",
      "domexception": "4.0.0",
      "stable": "0.1.8",
      "vue": "2.7.16",
      "webpack-chain": "6.5.1"
    }
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@8.15.3"
}
